{
  "name": "@generative-music/samples-alex-bainter",
  "version": "1.1.0",
  "description": "Audio sample files used in Alex Bainter's projects",
  "main": "src/index.js",
  "files": [
    "dist/index.json"
  ],
  "scripts": {
    "test": "mocha",
    "test:watch": "mocha -w",
    "lint": "eslint .",
    "clean": "rm -rf ./dist",
    "build": "node ./scripts/build/build.js",
    "build:clean": "npm run clean && npm run build",
    "pull": "node -r dotenv/config node_modules/.bin/s3-sync pull --mutables index.json",
    "deploy": "node -r dotenv/config node_modules/.bin/s3-sync deploy --mutables index.json --compressExtensions json,wav",
    "postversion": "git push && git push --tags",
    "preversion": "npm run lint && npm run test",
    "serve": "docker-compose up"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/generative-music/samples-alex-bainter.git"
  },
  "author": "Alex Bainter <alex@alexbainter.com> (https://alexbainter.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/generative-music/samples-alex-bainter/issues"
  },
  "homepage": "https://github.com/generative-music/samples-alex-bainter#readme",
  "devDependencies": {
    "@alexbainter/s3-sync": "^0.1.2",
    "@ffmpeg-installer/ffmpeg": "^1.0.20",
    "chai": "^4.2.0",
    "cli-progress": "^3.6.0",
    "concat-stream": "^2.0.0",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-config-metalex9": "^0.4.0",
    "fluent-ffmpeg": "^2.1.2",
    "md5": "^2.2.1",
    "mocha": "^7.0.1",
    "node-glob": "^1.2.0",
    "prettier": "^1.19.1",
    "proxyquire": "^2.1.3"
  }
}
